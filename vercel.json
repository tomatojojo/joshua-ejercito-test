{
    "version": 2,
    "builds": [
      {
        "src": "front/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "api/src/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/front/$1"
      }
    ]
  }
  